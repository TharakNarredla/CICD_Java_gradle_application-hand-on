pipeline{
    agent any 
    
    stages{
        stage ("sonar quality check"){
            agent any {
                steps{
                script{
                    withSonarQubeEnv(credentialsId: 'sonar-jenkinstocken') {
                            sh 'chmod +x gradlew'
                            sh './gradlew sonarqube'
                    }
                    }
                }
            }
        }
    }
}
